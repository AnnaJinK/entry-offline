{
  "env": {
    "development": {
      "application/javascript": {
        "presets": [
          [
            "@babel/preset-env",
            "@babel/preset-react",
            {
              "targets": {
                "electron": 4
              }
            }
          ]
        ],
        "plugins": [
          "@babel/plugin-syntax-dynamic-import",
          "@babel/plugin-transform-runtime",
          "@babel/plugin-proposal-class-properties",
          "async-import"
        ],
        "sourceMaps": "inline"
      }
    },
    "production": {
      "application/javascript": {
        "presets": [
          [
            "@babel/preset-env",
            "@babel/preset-react",
            {
              "targets": {
                "electron": 4
              }
            }
          ]
        ],
        "plugins": [
          "@babel/plugin-syntax-dynamic-import",
          "@babel/plugin-transform-runtime",
          "@babel/plugin-proposal-class-properties",
          "async-import"
        ],
        "sourceMaps": "none"
      }
    }
  }
}
