{
  "private": true,
  "name": "Entry",
  "productName": "Entry",
  "scripts": {
    "clean": "rimraf dist",
    "dist": "build",
    "comp": "electron-compile ./src/renderer/src ./src/main ./src/entry-offline.js",
    "dist:win": "electron-forge package --arch ia32",
    "dist:win64": "electron-forge package --arch x64",
    "pack:win": "electron-packager ./app Entry --platform=win32 --arch=ia32 --icon=./build/icon.ico --asar=true --overwrite=true",
    "pack:win64": "electron-packager ./app Entry --platform=win32 --arch=x64 --icon=./build/icon.ico",
    "start": "electron-forge start",
    "test": "mocha test/default.js",
    "package": "electron-forge package",
    "pack": "electron-forge package",
    "make": "electron-forge make",
    "preinstall": "bower cache clean && bower install"
  },
  "devDependencies": {
    "babel-plugin-transform-async-to-generator": "^6.22.0",
    "babel-preset-babili": "0.0.12",
    "babel-preset-env": "^1.2.1",
    "babel-preset-react": "^6.23.0",
    "electron-compilers": "^5.5.1",
    "electron-prebuilt-compile": "1.4.15",
    "mocha": "^2.5.3",
    "rimraf": "^2.5.2",
    "spectron": "^3.2.3"
  },
  "dependencies": {
    "bindings": "^1.2.1",
    "debug": "^2.6.3",
    "electron-compile": "^6.1.3",
    "es6-promise": "^4.1.0",
    "fs-extra": "^2.1.1",
    "fstream": "^1.0.11",
    "image-size": "^0.5.1",
    "jszip": "^3.1.3",
    "lodash": "^4.17.4",
    "mime-types": "^2.1.14",
    "object.assign": "^4.0.4",
    "q": "^1.4.1",
    "socket.io": "^1.7.3",
    "socket.io-client": "^1.7.3",
    "tar": "^2.2.1",
    "xml2js": "^0.4.17"
  },
  "main": "src/entry_offline.js",
  "version": "1.5.0",
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "zip"
        ],
        "darwin": [
          "dmg"
        ]
      },
      "electronPackagerConfig": {
        "icon": "build/icon",
        "asar": false,
        "win32metadata": {
          "CompanyName": "EntryLabs",
          "FileDescription": "Entry Offline Version",
          "OriginalFilename": "Entry.exe",
          "ProductName": "Entry",
          "InternalName": "Entry"
        }
      },
      "electronInstallerDMG": {
        "name": "Entry",
        "title": "Entry",
        "contents": [
          {
            "x": 240,
            "y": 380,
            "type": "link",
            "path": "/Applications"
          },
          {
            "x": 240,
            "y": 120,
            "type": "file",
            "path": "/Users/Naver/entry-offline/out/Entry-darwin-x64/Entry.app"
          },
          {
            "x": 380, 
            "y": 250,
            "type": "file",
            "path": "/Users/Naver/entry-offline/src/README.md"
          }
        ],
        "osxSign": {
          "identity": "Entry Education Laboratory INC."
        },
        "icon": "./build/icon.icns",
        "background": "./build/background.png",
        "extendInfo": "./build/build.plist",
        "icon-size": 80
      }
    }
  }
}
